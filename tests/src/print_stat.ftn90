program tests
  use vGrid_Descriptors, only: grid_descriptor,gd_new,gd_print
  use Unit_Testing, only: ut_report

  implicit none

  type(grid_descriptor) :: d
  integer :: stat

  ! Construct a new set of 3D coordinate descriptors
  stat = gd_new(d,unit=10,format='fst',ip1=100,ip2=200)

  ! Print information about the instance
  stat = gd_print(d)
  call ut_report(stat,message='Grid_Descriptors::gd_print dump output')

end program tests
