program tests
  use Grid_Descriptors, only: grid_descriptor,gd_new,gd_put,GD_LEN_NAME
  use Unit_Testing, only: ut_report

  implicit none

  type(grid_descriptor) :: d
  integer :: stat,fstkey
  real, dimension(:,:,:), allocatable :: lev
  character(len=GD_LEN_NAME) :: vname

  ! Construct a new set of 3D coordinate descriptors
  stat = gd_new(d,unit=10,format='fst',ip1=100,ip2=200)

  ! Change an element of the structure
  stat = gd_put(d,key='NAME - vertical coordinate name',value='VCRD',dir='z')
  call ut_report(stat,message='Grid_Descriptors::gd_put put character value')

end program tests
